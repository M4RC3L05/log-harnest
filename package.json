{
  "name": "log-harnest",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "xo": {
    "prettier": true,
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "sort-imports": [
        "error",
        {
          "allowSeparatedGroups": true
        }
      ],
      "unused-imports/no-unused-imports": "error",
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            [
              "internal",
              "sibling",
              "parent",
              "index"
            ],
            "object",
            "type"
          ],
          "distinctGroup": false,
          "newlines-between": "always",
          "pathGroups": [
            {
              "pattern": "#src/**",
              "group": "internal"
            }
          ]
        }
      ]
    },
    "ignorePatterns": [
      "node_modules"
    ]
  },
  "prettier": {
    "editorconfig": true,
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 120,
    "proseWrap": "preserve",
    "quoteProps": "consistent",
    "semi": true,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.0",
    "@types/koa-basic-auth": "^2.0.4",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^18.11.19",
    "eslint-plugin-unused-imports": "^2.0.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.3",
    "xo": "^0.53.1"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@leafac/sqlite": "^4.0.0",
    "bootstrap": "^5.3.0-alpha1",
    "bootstrap-icons": "^1.10.3",
    "config": "^3.3.9",
    "htm": "^3.1.1",
    "koa": "^2.14.1",
    "koa-basic-auth": "^4.0.0",
    "koa-static": "^5.0.0",
    "pino": "^8.9.0",
    "preact": "^10.12.0",
    "react-inspector": "^6.0.1",
    "serialize-error": "^11.0.0"
  },
  "engines": {
    "node": "18.*.*"
  }
}
